

/* -----------------------------查志磊--------------------- */

.map {  
    border: none;
    margin: 0;
    padding: 0;
    height: 100%;
    display: block;
  }
  
  
  .map-bar {
    position: relative;
    bottom: 82px;
    margin: 10px;
  }
  
  
  .map-bar.status {
    left: -16px;
    line-height: 16px;
  }
  
  .map-bar.status li {
    height: 16px;
    font-size: 16px;
    margin-top: 5px;
    float: right;
    width: 100%;
  }
  
  .map-bar.status .normal {
    color: #2bcf38;
  }
  
  .map-bar.status .warm {
    color: #febd00;
  }
  
  .map-bar.status .error {
    color: #eb2d2c;
  }
  
  .map-bar.video-list {
    background-color: rgba(0, 0, 0, 0.3);
    right: 0;
    padding: 10px;
  }
  
  .map-bar.video-list .video {
    width: 120px;
    height: 90px;
    float: right;
    margin-left: 5px;
    background-color: red;
  }
  
  
  /* -----------------------------查志磊--------------------- */