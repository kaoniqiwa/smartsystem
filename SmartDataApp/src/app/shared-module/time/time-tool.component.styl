.time-wraper
    height: 46px
    border-radius: 100px
    width: 300px
    padding: 10px 20px
    background-color: #2D3559

.left-wraper-label
   color: white
   font-size: 18px
   margin: 0
   padding-right: 10px
   border-right: 1px #3c4679 solid

.right-wraper-label
   color: #D0D7FF
   font-size: 16px
   margin: 0
   position: relative
   top: -2px
   margin-left: 10px
 

.colon-font
    position relative
    top -1px