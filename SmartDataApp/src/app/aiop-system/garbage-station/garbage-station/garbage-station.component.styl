.card-wrap
  background-color: alpha(#2448a5,0.05) 
  border: 2px  alpha(#7586e0,0.15) solid
  height  100%
  padding: 20px

.parent-node
  padding-left: 10px
  width: 100%
  height: 35px;
  padding-top: 5px
  cursor: pointer
 
 .default-label
  position: relative
  top: 4px
  cursor: pointer
  font-size: 18px
  line-height: 1

.default-label-width 
  max-width: 98%
  text-overflow: ellipsis
  overflow: hidden
  white-space: nowrap

.mask-chart
  height: 471px
  position: absolute
  z-index: 48
  background-color: alpha(#000,0)